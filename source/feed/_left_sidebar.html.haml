.flex-cell.flex-cell--left.flex-cell--side
  %div
    .feed-filter__profile
      %a.feed-filter__profile-edit{:href => "/user/profile/edit"} Edit profile
      .feed-filter__avatar
        PT
    .feed-filter
      %h4.feed-filter__title.feed-filter__title--feed
        My feed
      %ul.feed-filter__list
        %li.feed-default.feed-filter__item
          %a.feed-filter__link{:href => "javascript:void(0)"}
            View my feed
    .feed-filter
      %h4.feed-filter__title.feed-filter__title--groups
        Groups
      %ul.feed-filter__list.nav__groups
        %li No groups
      %button.btn-beige.btn-new{"data-target" => "#group-create-modal", "data-toggle" => "modal"} Create group
